<div class="author">
    <h1 class="author__heading">Welcome</h1>
    <p class="author__body">Just a simple blog of code learner.</p>
  </div>
  
  <img src="/img/stable-code-header.jpg" alt="learning code" class="hero-image" width="981" height="528">
  
  <section class="articles"> <a href="<%=  %>"
    <h2 class="articles__heading"> Posts</h2>


    <ul class="article-ul">
      <% data.forEach(post => { %>
      <li>
        <a href="/post/<%= post._id %>">
          <span><%= post.title %></span>
          <span class="article-list__date"><%= post.createdAt.toDateString() %><</span>
        </a>
      </li>

      <% }) %>

    </ul>
  
    <% if (nextPage !== null) { %>
      <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Posts</a>
    <% } %>
   
  
  
  
  </section>